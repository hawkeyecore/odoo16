<odoo>

    <record id="category_management_roles" model="ir.module.category">
        <field name="name">Manager</field>
        <field name="sequence" eval="10"/>
        <field name="visible" eval="True"/>
    </record>
    <record id="category_customer_roles" model="ir.module.category">
        <field name="name">Custom</field>
        <field name="sequence" eval="10"/>
        <field name="visible" eval="True"/>
    </record>
    <record id="category_Employee_roles" model="ir.module.category">
        <field name="name">Employee</field>
        <field name="sequence" eval="10"/>
        <field name="visible" eval="True"/>
    </record>
    <record id="scheduling_dispatch.group_manager" model="res.groups">
        <field name="name">Manager</field>
        <field name="category_id" ref="category_management_roles"/>
    </record>
    <record id="scheduling_dispatch.group_employee" model="res.groups">
        <field name="name">Employee</field>
        <field name="category_id" ref="category_Employee_roles"/>
    </record>
    <record id="scheduling_dispatch.group_customer" model="res.groups">
        <field name="name">Customer</field>
        <field name="category_id" ref="category_customer_roles"/>
    </record>
