<?xml version="1.0" encoding="UTF-8" ?>
<odoo>

    <record id="res_config_settings_view_form_inherit" model="ir.ui.view">
        <field name="name">res.config.settings.view.form.inherit.base.setup.inherit</field>
        <field name="model">res.config.settings</field>
        <field name="inherit_id" ref="base_setup.res_config_settings_view_form"/>
        <field name="arch" type="xml">
            <xpath expr="//div[@id='languages']" position="replace"/>
            <xpath expr="//div[@id='contacts_settings']" position="replace"/>
            <xpath expr="//div[@id='user_default_rights']" position="replace"/>
            <xpath expr="//div[@name='performance']" position="replace"/>
            <xpath expr="//div[@name='integration']" position="replace"/>
            <xpath expr="//div[@id='about']" position="replace"/>

        </field>
    </record>
</odoo>